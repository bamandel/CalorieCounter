<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.JNJABA.caloriecounter"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.JNJABA.caloriecounter.MainMenuActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.JNJABA.caloriecounter.JustAteActivity"
            android:label="@string/title_activity_food_log" >
        </activity>
        <activity
            android:name="com.JNJABA.caloriecounter.NewFoodActivity"
            android:label="@string/title_activity_new_food" >
        </activity>
        <activity
            android:name="com.JNJABA.caloriecounter.NewMealActivity"
            android:label="@string/title_activity_new_meal" >
        </activity>
        <activity
            android:name="com.JNJABA.caloriecounter.HealthActivity"
            android:label="@string/title_activity_health" >
        </activity>
        <activity
            android:name="com.JNJABA.caloriecounter.SelectFoodActivity"
            android:label="@string/title_activity_select_food" >
        </activity>
        <activity
            android:name="com.JNJABA.caloriecounter.SelectMealActivity"
            android:label="@string/title_activity_select_meal" >
        </activity>
        <activity
            android:name="com.JNJABA.caloriecounter.ProfileActivity"
            android:label="@string/title_activity_profile" >
        </activity>
    </application>

</manifest>
